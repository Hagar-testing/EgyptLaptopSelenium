<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" parallel="methods" thread-count="10">

    <listeners>
        <listener class-name="com.egyptlaptop.listeners.TestngListener"/>
    </listeners>

    <test name="search">
        <classes>
            <class name="com.egyptlaptop.testcases.MainTest"/>
        </classes>
    </test>

    <test name="cart">
        <classes>
            <class name="com.egyptlaptop.testcases.CartTest"/>
        </classes>
    </test>


    <test name="checkout">
        <classes>
            <class name="com.egyptlaptop.testcases.CheckoutTest"/>
        </classes>
    </test>

</suite>